{
  "name": "scout-vitepress-theme",
  "version": "0.1.1",
  "type": "module",
  "files": [
    "public",
    "src"
  ],
  "main": "src/main.ts",
  "exports": {
    ".": "./src/main.ts"
  },
  "scripts": {
    "start": "vitepress dev demo",
    "build:demo": "vitepress build demo",
    "serve": "npm run build:demo && vitepress serve demo",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vuetify": "^3.3.0"
  },
  "devDependencies": {
    "@mdi/js": "^7.2.96",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.1.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vite-plugin-vuetify": "^1.0.2",
    "vitepress": "^1.0.0-rc.15",
    "vitest": "^0.32.2",
    "vue-tsc": "^1.0.8"
  },
  "peerDependencies": {
    "vitepress": "^1.0.0-rc.15"
  }
}
